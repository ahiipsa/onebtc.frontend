

.DashboardCard {
  min-width: 346px;
  max-width: 346px;
  height: 346px;
  width: 346px;
  margin: 4px;
}

.circled {
  border: 4px solid #c2c2c2;
  border-radius: 50%;
  height 256px;
  width 256px;

  &.successBorder {
    border-color: #4ae3a7;
  }

  &.errorBorder {
    border-color: #FF0000;
  }
}

.success {
  color: #4ae3a7;
}

.error {
  color: #FF0000
}